@foreach (var inv in Inventories)
{
	<tr class="table">
		<td>@inv.Name</td>
		<td>@inv.Quantity</td>
		<td>@inv.Price.ToString("C")</td>
		<td>
			<button type="button"
					class="btn btn-light">
				Edit
			</button>
		</td>
	</tr>
}

@code {
	[Parameter]
	public List<Inventory> Inventories { get; set; }
}
